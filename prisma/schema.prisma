generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Friend {
  id                 Int     @id @default(autoincrement())
  name               String
  title              String?
  phoneNumber        String?
  email              String  @unique
  xUsername          String?
  instagramUsername  String?
}
